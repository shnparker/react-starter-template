{
  "name": "vite-starter-template",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:shnparker/vite-starter-template.git"
  },
  "author": {
    "name": "Shane Parker",
    "email": "hey@shnparker.com",
    "url": "https://shnparker.com",
    "github": "shnparker"
  },
  "description": "A template allowing for rapid development of React SPA projects based on Vite.",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write *.{js,jsx,ts,tsx,json,css,scss,md,yml,md} --loglevel warn",
    "format:check": "prettier --check *.{js,jsx,ts,tsx,json,css,scss,md,yml,md,mdx} --loglevel warn",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepush": "yarn lint && yarn format && yarn test",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.2"
  },
  "devDependencies": {
    "@headlessui/react": "1.7.13",
    "@heroicons/react": "2.0.16",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "lint-staged": "13.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,css,scss,md,yml,md}": "prettier --write"
  }
}
