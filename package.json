{
  "name": "react-starter-template",
  "private": true,
  "license": "MIT",
  "author": "Shane Parker",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "A template allowing for rapid development of React SPA projects based on Vite.",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write . --loglevel warn",
    "format:check": "prettier --check . --loglevel warn",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepush": "yarn test && yarn lint && yarn format",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "4.24.6",
    "axios": "1.3.3",
    "clsx": "1.2.1",
    "formik": "2.2.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "6.8.1",
    "yup": "1.0.0"
  },
  "devDependencies": {
    "@headlessui/react": "1.7.11",
    "@heroicons/react": "2.0.15",
    "@tailwindcss/forms": "0.5.3",
    "@tanstack/react-query-devtools": "4.24.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.28.5",
    "autoprefixer": "10.4.13",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "lint-staged": "13.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.3",
    "tailwindcss": "3.2.6",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,json,css,scss,md,yml,md}": "prettier --write"
  }
}
